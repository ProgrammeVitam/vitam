{
  "service": {
    "name": "{{ mongo_cluster_name }}-mongoc",
    "address": "{{ ip_service }}",
    "port": {{ mongodb.mongoc_port }},
    "enable_tag_override": false,
    "tags": ["bdd","mongo","{{ mongo_cluster_name }}","{{ mongo_cluster_name }}-mongoc"],
    "checks": [
      {
        "name": "{{ mongo_cluster_name }}-mongoc : TCP check",
        "notes": "Status {{ mongo_cluster_name }}-mongoc",
        "tcp": "{{ ip_service }}:{{ mongodb.mongoc_port }}",
        "interval": "{{ mongodb.check_consul }}s"
      }
    ]
  }
}
