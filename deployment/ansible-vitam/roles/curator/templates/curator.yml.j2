client:
  hosts:
    - {{ ip_admin }}
  port: {{ elasticsearch.log.port_http }}
  url_prefix:
  use_ssl: False
  certificate:
  client_cert:
  client_key:
  aws_key:
  aws_secret_key:
  aws_region:
  ssl_no_validate: False
  http_auth:
  timeout: 30
  master_only: False

logging:
  logfile: {{ vitam_defaults.folder.root_path | default('/vitam') }}/log/curator/curator.log
  loglevel: {{ curator.log_level | default('INFO') | upper }}
  logformat: default
  blacklist: ['elasticsearch', 'urllib3']
