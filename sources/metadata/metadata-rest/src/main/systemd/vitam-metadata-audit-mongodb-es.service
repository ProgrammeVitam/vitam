[Unit]
Description=Launch vitam audit data consistency for metadata

Wants=network.target
After=network.target

[Service]
Type=oneshot
EnvironmentFile=/vitam/conf/metadata/reconstruction.conf
ExecStart=/vitam/script/metadata/audit_data_consistency.sh
User=vitam
Group=vitam
StandardOutput=syslog
StandardError=syslog

[Install]
WantedBy=multi-user.target
