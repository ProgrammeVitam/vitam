{
  "$threshold": 10,
  "$queries": [
    {
      "$query": [
        {
          "$or": [
            {
              "$in": {
                "operations": [
                  "Operation-Id"
                ]
              }
            },
            {
              "$in": {
                "#allunitups": []
              }
            }
          ],
          "$depth": 1000
        }
      ],
      "$action": [
        {
          "$setregex": {
            "$target": "_sedaVersion",
            "$controlPattern": "YEAH",
            "$updatePattern": "Toto"
          }
        }
      ]
    }
  ]
}