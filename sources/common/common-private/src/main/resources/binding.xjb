<jxb:bindings version="2.0"
              xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance"
              xmlns:annox="http://annox.dev.java.net"
              xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <jxb:bindings schemaLocation="http://www.w3.org/1999/xlink.xsd" node="/xs:schema">

        <jxb:bindings node="//xs:attributeGroup[@name='locatorAttrs']">
            <jxb:bindings node=".//xs:attribute[@ref='xlink:title']">
                <jxb:property name="title1"/>
            </jxb:bindings>
        </jxb:bindings>

        <jxb:bindings node="//xs:attributeGroup[@name='arcAttrs']">           
            <jxb:bindings node=".//xs:attribute[@ref='xlink:title']">
                <jxb:property name="title2"/>
            </jxb:bindings>            
        </jxb:bindings>              
    </jxb:bindings>

    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-descriptive.xsd" node="/xs:schema">

        <jxb:bindings node="xs:complexType[@name='ArchiveUnitType']">
            <jxb:bindings node="//xs:element[@name='ArchiveUnitRefId']">
                <jxb:property>
                    <jxb:baseType>
                        <jxb:javaType name="java.lang.String"></jxb:javaType>
                    </jxb:baseType>
                </jxb:property>
            </jxb:bindings>
            <annox:annotate>
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement"
                                name="ArchiveUnit" />
            </annox:annotate>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-main.xsd" node="/xs:schema">

        <jxb:bindings node="xs:complexType[@name='DataObjectPackageType']">
            <annox:annotate>
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement"
                                name="DataObjectPackage" />
            </annox:annotate>
        </jxb:bindings>
        <jxb:bindings node="xs:complexType[@name='ArchiveTransferReplyType']">
            <annox:annotate>
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement"
                                name="ArchiveTransferReply"/>
            </annox:annotate>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-technical.xsd" node="/xs:schema">

        <jxb:bindings node="xs:complexType[@name='BinaryDataObjectType']">
            <annox:annotate>
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement"
                                name="BinaryDataObject"/>
            </annox:annotate>

        </jxb:bindings>
        <jxb:bindings node="xs:complexType[@name='PhysicalDataObjectType']">
            <annox:annotate>
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement"
                                name="PhysicalDataObject"/>
            </annox:annotate>

        </jxb:bindings>
    </jxb:bindings>



    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-management.xsd">
        <jxb:bindings node="//xs:complexType[@name='StorageRuleType']">
            <inheritance:implements>fr.gouv.vitam.common.model.unit.CommonRule</inheritance:implements>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='AppraisalRuleType']">
            <inheritance:implements>fr.gouv.vitam.common.model.unit.CommonRule</inheritance:implements>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='AccessRuleType']">
            <inheritance:implements>fr.gouv.vitam.common.model.unit.CommonRule</inheritance:implements>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='DisseminationRuleType']">
            <inheritance:implements>fr.gouv.vitam.common.model.unit.CommonRule</inheritance:implements>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='ReuseRuleType']">
            <inheritance:implements>fr.gouv.vitam.common.model.unit.CommonRule</inheritance:implements>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='ClassificationRuleType']">
            <inheritance:implements>fr.gouv.vitam.common.model.unit.CommonRule</inheritance:implements>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-types.xsd">
        <jxb:bindings node="//xs:element[@name='DataObjectReferenceId']">
            <jxb:property>
                <jxb:baseType>
                    <jxb:javaType name="java.lang.String"></jxb:javaType>
                </jxb:baseType>
            </jxb:property>
        </jxb:bindings>
        <jxb:bindings node="//xs:element[@name='DataObjectGroupReferenceId']">
            <jxb:property>
                <jxb:baseType>
                    <jxb:javaType name="java.lang.String"></jxb:javaType>
                </jxb:baseType>
            </jxb:property>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-technical.xsd">
        <jxb:bindings node="//xs:element[@name='DataObjectGroupReferenceId']">
            <jxb:property>
                <jxb:baseType>
                    <jxb:javaType name="java.lang.String"></jxb:javaType>
                </jxb:baseType>
            </jxb:property>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="seda-vitam/seda-vitam-ontology.xsd">
        <jxb:bindings node="//xs:element[@name='SignedObjectId']">
            <jxb:property>
                <jxb:baseType>
                    <jxb:javaType name="java.lang.String"></jxb:javaType>
                </jxb:baseType>
            </jxb:property>
        </jxb:bindings>
        <jxb:bindings node="xs:complexType[@name='DataObjectOrArchiveUnitReferenceType']">
            <jxb:bindings node="//xs:element[@name='ArchiveUnitRefId']">
                <jxb:property>
                    <jxb:baseType>
                        <jxb:javaType name="java.lang.String"></jxb:javaType>
                    </jxb:baseType>
                </jxb:property>
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='AgentType']">
            <jxb:class ref="fr.gouv.vitam.common.model.unit.AgentTypeModel" />
        </jxb:bindings>
        <jxb:bindings node="//xs:element[@name='Validator']">
            <jxb:class ref="fr.gouv.vitam.common.model.unit.ValidatorOrSignerModel" />
        </jxb:bindings>
        <jxb:bindings node="//xs:element[@name='Signer']">
            <jxb:class ref="fr.gouv.vitam.common.model.unit.ValidatorOrSignerModel" />
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='PersistentIdType']">
            <jxb:class ref="fr.gouv.vitam.common.model.unit.PersistentIdentifierModel" />
        </jxb:bindings>
        <!--jxb:bindings node="//xs:element[@name='Writer']">
            <jxb:class ref="fr.gouv.vitam.common.model.unit.WriterModel" />
        </jxb:bindings-->

    </jxb:bindings>

</jxb:bindings>
