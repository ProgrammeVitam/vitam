{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://example.com/root.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "ObjectSize": {
      "type": "object",
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "_id": {
      "type": "string"
    },
    "_tenant": {
      "type": "integer"
    },
    "_v": {
      "type": "integer"
    },
    "OriginatingAgency": {
      "type": "string"
    },
    "TotalObjects": {
      "type": "object",
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "TotalObjectGroups": {
      "type": "object",
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "TotalUnits": {
      "type": "object",
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "CreationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    }
  }
}
