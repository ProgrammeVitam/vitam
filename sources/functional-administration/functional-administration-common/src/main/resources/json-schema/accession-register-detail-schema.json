{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "date-opt-time": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    }
  },
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "ObjectSize": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "_id": {
      "type": "string"
    },
    "_tenant": {
      "type": "integer"
    },
    "_v": {
      "type": "integer"
    },
    "Opc": {
      "type": "string"
    },
    "Opi": {
      "type": "string"
    },
    "OpType": {
      "type": "string"
    },
    "OriginatingAgency": {
      "type": "string"
    },
    "SubmissionAgency": {
      "type": "string"
    },
    "ArchivalProfile": {
      "type": "string"
    },
    "obIdIn": {
      "type": "string"
    },
    "Comment": {
      "type": "array",
      "items": {
        "type": "string"
       }
    },
    "ArchivalAgreement": {
      "type": "string"
    },
    "AcquisitionInformation": {
      "type": "string"
    },
    "LegalStatus": {
      "type": "string"
    },
    "EndDate": {
      "$ref": "#/definitions/date-opt-time"
    },
    "StartDate": {
      "$ref": "#/definitions/date-opt-time"
    },
    "LastUpdate": {
      "$ref": "#/definitions/date-opt-time"
    },
    "Status": {
      "type": "string",
      "enum": [
        "STORED_AND_COMPLETED",
        "STORED_AND_UPDATED",
        "UNSTORED"
      ]
    },
    "TotalObjectGroups": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "TotalUnits": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "TotalObjects": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ingested": {
          "type": "number"
        },
        "deleted": {
          "type": "number"
        },
        "remained": {
          "type": "number"
        }
      }
    },
    "OperationIds": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "Events": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "Opc": {
            "type": "string"
          },
          "OpType": {
            "type": "string"
          },
          "Gots": {
            "type": "number"
          },
          "Units": {
            "type": "number"
          },
          "Objects": {
            "type": "number"
          },
          "ObjSize": {
            "type": "number"
          },
          "CreationDate": {
            "$ref": "#/definitions/date-opt-time"
          }
        }
      }
    }
  },
  "required": [
    "_id",
    "_tenant",
    "_v",
    "OriginatingAgency",
    "Opc",
    "Opi",
    "Events",
    "TotalObjectGroups",
    "TotalUnits",
    "TotalObjects",
    "ObjectSize"
  ]
}
