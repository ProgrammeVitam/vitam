{
  "id": "AUDIT_WORKFLOW",
  "name": "Default Audit Object Workflow",
  "identifier": "PROCESS_AUDIT",
  "typeProc": "AUDIT",
  "comment": "Default Audit Object workflow V1",
  "lifecycleLog": "DISABLED",
  "steps": [
    {
      "workerGroupId": "DefaultWorker",
      "stepName": "STP_PREPARE_AUDIT",
      "behavior": "BLOCKING",
      "distribution": {
        "kind": "REF",
        "element": ""
      },
      "actions": [
        {
          "action": {
            "actionKey": "LIST_OBJECTGROUP_ID",
            "behavior": "BLOCKING"
          }
        }
      ]
    },
    {
      "workerGroupId": "DefaultWorker",
      "stepName": "STP_AUDIT",
      "behavior": "BLOCKING",
      "distribution": {
        "kind": "LIST_IN_JSONL_FILE",
        "element": "AUDIT_OG",
        "type" : "ObjectGroup"
      },
      "actions": [
        {
          "action": {
            "actionKey": "AUDIT_CHECK_OBJECT",
            "behavior": "NOBLOCKING",
            "out": [
              {
                "name": "shouldWriteLogbook",
                "uri": "MEMORY:shouldWrite"
              }
            ]
          }
        }
      ]
    },
    {
      "workerGroupId": "DefaultWorker",
      "stepName": "STP_FINALISE_AUDIT",
      "behavior": "BLOCKING",
      "distribution": {
        "kind": "REF",
        "element": ""
      },
      "actions": [
        {
          "action": {
            "actionKey": "REPORT_AUDIT",
            "behavior": "BLOCKING"
          }
        }
      ]
    }
  ]
}
